@model WebApplication1.Licence

@{
    ViewData["Title"] = "Details";
}

<h2>Информация о лицензии</h2>
<div style="margin-bottom:36.1vh">
    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LicenceNumber)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LicenceNumber)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LicenceDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LicenceDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ExpireDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ExpireDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Categories)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Categories)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LicenceSeries)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LicenceSeries)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdDriverNavigation)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.IdDriverNavigation.Name)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.LicenceNumber">Редактировать</a> |
        <a asp-action="Index">Вернуться к списку</a>
    </div>
</div>
